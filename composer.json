{
  "name": "tourze/jingdong-crm-bundle",
  "description": "京东CRM",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "symfony/config": "^6.4",
    "symfony/dependency-injection": "^6.4",
    "symfony/framework-bundle": "^6.4",
    "symfony/http-kernel": "^6.4",
    "symfony/routing": "^6.4",
    "symfony/yaml": "^6.4 || ^7.1",
    "tourze/symfony-integration-test-kernel": "0.0.*"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "JingdongCrmBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "JingdongCrmBundle\\Tests\\": "tests"
    }
  }
}