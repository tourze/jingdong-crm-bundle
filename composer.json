{
  "name": "tourze/jingdong-crm-bundle",
  "description": "京东CRM",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "symfony/config": "^6.4",
    "symfony/dependency-injection": "^6.4",
    "symfony/http-kernel": "^6.4",
    "symfony/framework-bundle": "^6.4",
    "tourze/easy-admin-attribute": "0.0.*",
    "tourze/http-client-bundle": "0.0.*"
  },
  "autoload": {
    "psr-4": {
      "JingdongCrmBundle\\": "src"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "phpstan/phpstan": "^2.1"
  },
  "scripts": {
    "phpstan": "phpstan analyse src -l 1"
  }
}